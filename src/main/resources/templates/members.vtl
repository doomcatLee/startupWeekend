<!--category.vtl-->
<h1>$team.getName() Members:</h1>
<ul>
  #foreach($member in $team.getMembers())
   <a href="/teams/$team.getId()/members/$team.getMemberId($member)"><li>$member.getName()</li></a>
  #end
</ul>

<button class="btn btn-default"><a href="/teams/$team.getId()/members/new">Add a new member</a></button>
<button class="btn btn-default"><a href="/teams">View all Teams</a></button>
<!--category.vtl-->
